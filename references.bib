@article{leung-gagne2023,
  title = {It {{Is Surprisingly Difficult}} to {{Measure Income Segregation}}},
  author = {{Leung-Gagn{\'e}}, Josh and Reardon, Sean F.},
  year = {2023},
  month = aug,
  journal = {Demography},
  pages = {10932629},
  issn = {0070-3370, 1533-7790},
  doi = {10.1215/00703370-10932629},
  urldate = {2023-08-28},
  abstract = {Recent studi\-es have shown that U.S. Census{\textendash} and Ameri\-\-can Community Survey (ACS){\textendash}based esti\-ma\-tes of income seg\-re\-ga\-tion are sub\-ject to upward finite sam\- pling bias (Logan et al. 2018; Logan et al. 2020; Reardon et al. 2018). We ident\-ify two addi\-tional sources of bias that are larger and oppos\- ite in sign to finite samp\- ling bias: meas\- ure\-ment error{\textendash}induced attenu\- \-a\-tion bias and temp\- o\-ral pooling bias. The comb\- i\-na\- tion of these three sources of bias make it unclear how income seg\-re\-ga\-tion has trended. We form\- al\-ize the three types of bias, prov\- idi\-ng a method to corr\- ect them simul\-ta\-neously using publ\-ic data from the decenn\- ial cens\- us and ACS from 1990 to 2015{\textendash}2019. We use these meth\-ods to pro\-duce bias-corrected esti\-ma\-tes of income seg\-re\-ga\-tion in the United States from 1990 to 2019. We find that (1) segr\-e\-ga\-tion is on the order of 50\% greater than prev\- io\- usly believed; (2) the increase from 2000 to the 2005{\textendash}2009 period was much greater than indic\- ated by prev\- i\-ous esti\-ma\-tes; and (3) seg\-re\-ga\-tion has declined since 2005{\textendash}2009. Correcting these biases requires good estim\- a\-tes of the reli\-abil\-ity of self-reported income and of the year-to-year vola\- \-tili\-ty in neighb\- or\-hood mean incomes.},
  langid = {english},
  file = {C:\Users\csf14\Zotero\storage\3UUPBE6M\Leung-Gagné and Reardon - 2023 - It Is Surprisingly Difficult to Measure Income Seg.pdf}
}

