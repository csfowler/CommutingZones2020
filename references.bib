@article{card2007,
  title = {Racial Segregation and the Black{\textendash}White Test Score Gap},
  author = {Card, David and Rothstein, Jesse},
  year = {2007},
  month = dec,
  journal = {Journal of Public Economics},
  volume = {91},
  number = {11-12},
  pages = {2158--2184},
  publisher = {{North-Holland}},
  doi = {10.1016/j.jpubeco.2007.03.006},
  abstract = {Racial segregation is often blamed for some of the achievement gap between blacks and whites. We study the effects of school and neighborhood segregation on the relative SAT scores of black students across different metropolitan areas, using large microdata samples for the 1998{\textendash}2001 test cohorts. Our models include detailed controls for the family background of individual test-takers, school-level controls for selective participation in the test, and city-level controls for racial composition, income, and region. We find robust evidence that the black{\textendash}white test score gap is higher in more segregated cities. Holding constant family background and other factors, a shift from a highly segregated city to a nearly integrated city closes about one-quarter of the raw black{\textendash}white gap in SAT scores. Specifications that distinguish between school and neighborhood segregation suggest that neighborhood segregation has a consistently negative impact while school segregation has no independent effect, though we cannot reject equality of the two effects. Additional tests indicate that much of the effect of neighborhood segregation operates through neighbors' incomes, not through race per se. Data on enrollment in honors courses suggest that within-school segregation increases when schools are more highly integrated, potentially offsetting the benefits of school desegregation and accounting for our findings.},
  keywords = {\#nosource}
}

