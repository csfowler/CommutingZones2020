@article{lebeau,
  title = {A {{Guide}} to the {{American Community Survey}} ({{ACS}}) for the {{Rural Researcher}}: {{Unpacking}} the {{Conceptual}} and {{Technical Aspects}} of {{Using Secondary Data}} for {{Rural Research}}\ding{73}},
  shorttitle = {A {{Guide}} to the {{American Community Survey}} ({{ACS}}) for the {{Rural Researcher}}},
  author = {LeBeau, Kristie},
  journal = {Rural Sociology},
  volume = {n/a},
  number = {n/a},
  issn = {1549-0831},
  doi = {10.1111/ruso.12493},
  urldate = {2023-06-22},
  abstract = {Sparsely populated rural areas are susceptible to high levels of error in their data, making it difficult to examine patterns and trends across geographies. This article aims to advance research methods for rural researchers by offering guidelines for navigating high levels of error associated with the American Community Survey (ACS). The ACS presents a useful source of U.S. community level data for rural researchers to utilize in school{\textendash}community research but not without its difficulties. The small population sizes of rural communities often translate to large margins of error in the data, presenting a degree of uncertainty in the actual measure. To illustrate challenges and best practice, the author conducts a case study of the relationship between the presence of schools and economic vitality of rural communities in Indiana using ACS data. The author demonstrates how to examine the error in the data, introduces options to reduce uncertainty, and ultimately, explains how to move forward with the data, working with the margin of error and acknowledging its presence in the analysis and results. This article offers suggestions and techniques to assist rural researchers in navigating ACS obstacles so that they might produce transparent results with as little uncertainty as possible.},
  copyright = {{\textcopyright} 2023 Rural Sociological Society (RSS).},
  langid = {english},
  file = {C\:\\Users\\csf14\\Zotero\\storage\\WWDQKEMN\\LeBeau - A Guide to the American Community Survey (ACS) for.pdf;C\:\\Users\\csf14\\Zotero\\storage\\I7PE86M7\\ruso.html}
}

