@article{schroeder2021,
  title = {Across the {{Rural}}{\textendash}{{Urban Universe}}: {{Two Continuous Indices}} of {{Urbanization}} for {{U}}.{{S}}. {{Census Microdata}}},
  author = {Schroeder, Jonathan P and Pacas, Jose},
  year = {2021},
  journal = {Spatial Demography},
  volume = {9},
  number = {1},
  pages = {131--154},
  doi = {10.1007/s40980-021-00081-y},
  abstract = {Microdata from U.S. decennial censuses and the American Community Survey are a key resource for social science and policy analysis, enabling researchers to investigate relationships among all reported characteristics for individual respondents and their households. To protect privacy, the Census Bureau restricts the detail of geographic information in public use microdata, and this complicates how researchers can investigate and account for variations across levels of urbanization when analyzing microdata. One option is to focus on metropolitan status, which can be determined exactly for most microdata records and approximated for others, but a binary metro/nonmetro classification is still coarse and limited on its own, emphasizing one aspect of rural{\textendash}urban variation and discounting others. To address these issues, we compute two continuous indices for public use microdata{\textemdash}average tract density and average metro/micro-area population{\textemdash}using population-weighted geometric means. We show how these indices correspond to two key dimensions of urbanization{\textemdash}concentration and size{\textemdash}and we demonstrate their utility through an examination of disparities in poverty throughout the rural{\textendash}urban universe. Poverty rates vary across settlement types in nonlinear ways: rates are lowest in moderately dense parts of major metro areas, and rates are higher in both low- and high-density areas, as well as in smaller commuting systems. Using the two indices also reveals that correlations between poverty and demographic characteristics vary considerably across settlement types. Both indices are now available for recent census microdata via IPUMS USA (               https://usa.ipums.org                           ).},
  annotation = {MAG ID: 3136959273}
}

