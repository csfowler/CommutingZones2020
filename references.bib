@article{Steinle2013,
  title = {Quantifying Human Exposure to Air Pollution-{{Moving}} from Static Monitoring to Spatio-Temporally Resolved Personal Exposure Assessment},
  author = {Steinle, Susanne and Reis, Stefan and Sabel, Clive Eric},
