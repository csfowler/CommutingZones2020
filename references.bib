@article{asson2022,
  title = {Data {{Access}} and the {{Study}} of {{Educational Equity}}: {{Implications}} from a {{National School Boundary Data Collection Effort}}},
  shorttitle = {Data {{Access}} and the {{Study}} of {{Educational Equity}}},
  author = {Asson, Sarah and Frankenberg, Erica and Maselli, Annie and {Burfoot-Rochford}, Ian and Fowler, Christopher S. and Buck, Ruth Krebs},
  year = {2022},
