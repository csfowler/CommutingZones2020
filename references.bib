  file = {C\:\\Users\\csf14\\Zotero\\storage\\I35SSKGA\\Coe et al. - 2004 - 'Globalizing' regional development a global production networks perspective(2).pdf;C\:\\Users\\csf14\\Zotero\\storage\\LSH8W7EI\\Coe et al. - 2004 - 'Globalizing' regional development a global production networks perspective(2).pdf;C\:\\Users\\csf14\\Zotero\\storage\\UJLVFVJC\\Coe et al. - 2004 - 'Globalizing' regional development a global production networks perspective.pdf}
