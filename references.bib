@article{Sweeney1998,
  title = {Plant Size and Clustering of Manufacturing Activity},
  author = {Sweeney, Stuart H and Feser, Edward J},
  year = {1998},
  journal = {Sage Urban Studies Abstracts},
  volume = {26},
  number = {2},
  publisher = {{Sage Publications}},
  abstract = {At least two distinct but related bodies of theory and analysis emphasize the advantage of spatial clustering for smaller business establishments. Researchers studying trends in technological change and innovation have cited the role of information and knowledge exchange among co-located smaller firms, as well as between smaller firms and nearby research universities, as one viable substitute for the in-house research and development (RD) capacity of larger producers. However, there has been little empirical work that has directly examined clustering trends by firm size. This paper presents an empirical analysis of the isolated effects of size on clustering patterns of producers in a major manufacturing state in the southeastern United States. Recent developments in point process modeling allow for the control of the critical fact that economic activity is, in general, concentrated in space. These findings suggest that the relationship between establishment size and clustering in North Carolina is roughly characterized by an inverted U-shape, that is, clustering increases up to a certain threshold and then decreases again.},
  pmid = {87603128},
  keywords = {Economic clusters}
}

