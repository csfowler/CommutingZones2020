@article{crowder2016,
  title = {Moving {{Beyond}} the {{Big Three}}: {{A Call}} for {{New Approaches}} to {{Studying Racial Residential Segregation}}},
  author = {Crowder, Kyle and Krysan, Maria},
  year = {2016},
  month = mar,
  journal = {City \& Community},
  volume = {15},
  number = {1},
  pages = {18--22},
  issn = {15356841},
  doi = {10.1111/cico.12148},
  abstract = {Our essay begins from the premise that residential segregation by race remains a defining feature of most metropolitan areas and continues to exert strong and multifaceted im-pacts on the life chances of segregated populations. Despite declines in average levels of segregation and the increasing prevalence of multiethnic neighborhoods (Glaeser and Vigdor 2012), overall levels of segregation between black and white Americans remain high by most measures, and are especially pronounced in cities with the largest black pop-ulations (Massey and Tannen 2015; Massey 2015). Levels of segregation between Latinos and whites, and between Asians and whites, have changed very little in recent decades (Massey 2015). The consequences of this persistent segregation are vast and well documented. The central repercussion is that members of different racial and ethnic groups are still ex-posed, on average, to fundamentally unequal neighborhood environments. A huge body of research has linked this segregation to the persistence of profound racial differences in life chances as reflected in differential exposure to neighborhood poverty (Logan 2011), crime (Peterson and Krivo 2010), and pollution (Crowder and Downey 2010; Jones et al. 2014), access to educational and employment opportunities (De la Roca et al. 2014; Frankenberg 2013; Kneebone and Holmes 2015; Logan et al. 2012), and a wide range of individual outcomes, including earnings (Thomas and Moye 2015), wealth (Shapiro et al. 2013), and health (Kershaw and Albrecht 2015; Subramanian et al. 2005; White and Borrell 2011; Yang and Matthews 2015). These consequences of segregation extend well beyond those felt by segregated minorities. The more insidious effects of segregation are on the broader social, political, and economic health of the country, as evidenced most publicly in the ongoing unrest over police violence against people of color, the roots of which can be found in the profound racial injustices borne out of segregation and differ-ential opportunity structures. Yet, while evidence for the continuing salience of segregation is strong, our explana-tions for its persistence remain relatively weak. Our collective attempts to understand the root causes of segregation have fallen into a kind of intellectual malaise, characterized by the overly simplistic and completely automatic application of a few simple theoretical arguments. These arguments are incapable of explaining why segregation remains such 18 MOVING BEYOND THE BIG THREE a defining characteristic of most of our metropolitan areas, despite liberalizing racial at-titudes, reductions in the most overt forms of discrimination, economic advancement for some minority groups, and technological advances that should loosen the ties of neigh-borhood. Our explanations, in short, have become out-dated. In order to understand why segregation persists, then, we need to move in different directions theoretically and analytically. We must critically assess our current theoretical arguments, critique the as-sumptions underlying their application, consider factors not captured in these traditional arguments, and investigate ways to collect better data on the multifaceted factors that maintain residential stratification. Virtually all of the massive body of research on the patterns and drivers of segrega-tion relies on some combination of three dominant theories (Charles 2003). The first of these Big Three theoretical arguments focuses on racial differences in human capital and argues that higher levels of education, income, and wealth improve access to the most advantaged neighborhoods (Crowder et al. 2006; South and Crowder 1997) such that racial segregation arises from racial/ethnic differences in these resources. The sec-ond argument contends that residential segregation stems from racial/ethnic differences in residential preferences, with out-group avoidance or in-group affinity leading individ-uals to choose neighborhoods dominated by their own group (Charles 2006; Clark 2008; Krysan et al. 2009). Third, the place stratification perspective (Charles 2003; Logan and Molotch 1987) suggests that whites' aversion to sharing residential space with minor-ity neighbors reinforces discriminatory practices of real estate agents, landlords, mort-gage lenders, and neighbors (Roscigno et al. 2009; Ross and Turner 2005), which creates a racially-segmented housing market that produces disparate residential outcomes. In essence, the tendency is to assume that segregation comes about because members of different groups simply prefer different kinds of neighborhoods, some groups lack the resources needed to gain access to " better " neighborhoods, or discriminatory treatment in the housing market denies people the opportunity to live where they would like. There is ample research to suggest that the dynamics implicated in each of these Big Three theoretical arguments contribute to the maintenance of racial-residential strat-ification. However, the steadfast and exclusive reliance on these arguments and their somewhat simplistic application has hampered our ability to develop a stronger under-standing of the processes through which segregation is maintained and perpetuated. The way in which the field has relied on these dominant theories creates two core problems. First, the forces implicated in these theoretical arguments are often treated as compet-ing, mutually exclusive, and virtually exhaustive explanations of segregation, so much so that weak evidence in a study for one theory is interpreted as support for another of the theories. For example, if racial differences in the neighborhood attainment patterns that shape segregation persist after controls for group differences in socioeconomic condi-tions, the typical conclusion is that the remaining differences reflect some combination of discrimination and group differences in preferences (Crowder et al. 2006; Logan et al. 1996). This tendency to view existing arguments as competing and additive, of course, undermines any ability to recognize the ways in which preferences, discrimination, and economic forces interact and complement one another in the maintenance of residen-tial segregation, and closes off the assessment of more complex, but no less important, dynamics that maintain residential segregation. A second related problem is that implicit in the three dominant theories is the assump-tion that individuals possess accurate and complete information about the community 19},
  keywords = {\#nosource}
}

@article{crowe2007,
  title = {In Search of a Happy Medium: {{How}} the Structure of Interorganizational Networks Influence Community Economic Development Strategies},
  author = {Crowe, J A},
  year = {2007},
  journal = {Social Networks},
  volume = {29},
  number = {4},
  pages = {469--488},
  issn = {0378-8733},
  doi = {10.1016/j.socnet.2007.02.002},
  abstract = {Routes to economic development attract considerable attention among social scientists, policy makers, and community activists. Increasingly, social scientists examine various attributes of communities, their members, and their natural surroundings that facilitate and inhibit economic development. However, few empirical analyses exist that analyze the impact of a community's network structure on different forms of economic development such as on industrial recruitment and self-development. Using data collected from six communities in Washington State, the impact of a community's interorganizational network structure on industrial recruitment and self-development is examined. Results suggest that different types of network structures are better suited for different economic development strategies. A certain level of cohesiveness among community organizations and institutions are favorable for implementing self-development projects. However for industrial recruitment, networks that are bridging facilitate more types of economic development. While bonding and bridging network structures appear to be at odds with one another, it is possible for communities to increase both forms of economic development by maintaining a certain level of cohesiveness among subcomponents and increasing the number of organizations that serve as cut-points connecting non-redundant sources of information. These findings illustrate the need for communities and local activists to consider a community's network structure when deciding on an economic development strategy. (C) 2007 Elsevier B.V. All rights reserved.},
  keywords = {\#nosource,and bridging social capital,bonding,economic development,industrial recruitment,POLITICAL-ECONOMY,self-development,SOCIAL INFRASTRUCTURE}
}

