  volume = {39},
  number = {3},
  pages = {293--310},
  issn = {0016-7363},
  doi = {10.1111/j.1538-4632.2007.00705.x},
  abstract = {This article bridges the permutation test of Moran's I to the residuals of a loglinear model under the asymptotic normality assumption. It provides the versions of Moran's I based on Pearson residuals (IPR) and deviance residuals (IDR) so that they can be used to test for spatial clustering while at the same time account for potential covariates and heterogeneous population sizes. Our simulations showed that both IPR and IDR are effective to account for heterogeneous population sizes. The tests based on IPR and IDR are applied to a set of log-rate models for early-stage and late-stage breast cancer with socioeconomic and access-to-care data in Kentucky. The results showed that socioeconomic and access-to-care variables can sufficiently explain spatial clustering of early-stage breast carcinomas, but these factors cannot explain that for the late stage. For this reason, we used local spatial association terms and located four late-stage breast cancer clusters that could not be explained. The results also confirmed our expectation that a high screening level would be associated with a high incidence rate of early-stage disease, which in turn would reduce late-stage incidence rates.},
  keywords = {Residuals,Spatial autocorrelation,Spatial statistics},
  file = {C\:\\Users\\csf14\\Zotero\\storage\\8M2PHEVS\\Lin, Zhang - 2007 - Loglinear Residual Tests of Moran's I Autocorrelation and their Applications to Kentucky Breast Cancer Data.pdf;C\:\\Users\\csf14\\Zotero\\storage\\BHW96BSE\\2007-Loglinear_Residual_Tests_of_Morans_I_Autocorrelation_and_their_Applications_to_Kentucky_Breast_Cancer_Data.pdf;C\:\\Users\\csf14\\Zotero\\storage\\HCLH95S5\\2007-Loglinear_Residual_Tests_of_Morans_I_Autocorrelation_and_their_Applications_to_Kentucky_Breast_Cancer_Data.pdf;C\:\\Users\\csf14\\Zotero\\storage\\TFL7AZ8F\\Lin, Zhang - 2007 - Loglinear Residual Tests of Moran's I Autocorrelation and their Applications to Kentucky Breast Cancer Data.pdf}
