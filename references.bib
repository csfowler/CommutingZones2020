@article{seanf.reardon2016,
  title = {School {{Segregation}} and {{Racial Academic Achievement Gaps}}},
  author = {{Sean F. Reardon}},
  year = {2016},
  journal = {RSF: The Russell Sage Foundation Journal of the Social Sciences},
  volume = {2},
