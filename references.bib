@article{florez-revuelta2008,
  title = {An Evolutionary Approach to the Delineation of Functional Areas Based on Travel-to-Work Flows},
  author = {{Fl{\'o}rez-Revuelta}, Francisco and {Casado-D{\'i}az}, Jos{\'e} M. and {Mart{\'i}nez-Bernabeu}, Lucas},
  year = {2008},
