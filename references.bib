@article{Coe2004,
  title = {"\{\vphantom\}{{G}}\vphantom\{\}lobalizing" {{Regional Development}}: {{A Global Production Networks Perspective}}},
  author = {Coe, Neil M and Hess, Martin and Yeung, Henry Wai-chung and Disken, P and Henderson, Jeffrey and Dicken, Peter and Henderson, Jeffrey and Yeungt, Henry Wai-chung and Disken, P and Henderson, Jeffrey and Dicken, Peter and Henderson, Jeffrey and Disken, P and Henderson, Jeffrey},
  year = {2004},
  journal = {Transactions of the Institute of British Geographers},
  volume = {29},
  number = {4},
  pages = {468--484},
  issn = {0020-2754},
  doi = {10.1111/j.0020-2754.2004.00142.x},
  abstract = {Recent literature concerning regional development has placed significant emphasis on local institutional structures and their capacity to `hold down' the global. Conversely, work on inter-firm networks {\textendash} such as the global commodity chain approach {\textendash} has highlighted the significance of the organizational structures of global firms' production systems and their relation to industrial upgrading. In this paper, drawing upon a global production networks perspective, we conceptualize the connections between `globalizing' processes, as embodied in the production networks of transnational corporations, and regional development in specific territorial formations. We delimit the `strategic coupling' of the global production networks of firms and regional economies which ultimately drives regional development through the processes of value creation, enhancement and capture. In doing so, we stress the multi-scalarity of the forces and processes underlying regional development, and thus do not privilege one particular geographical scale. By way of illustration, we introduce an example drawn from recent research into global production networks in East Asia and Europe. The example profiles the investments of car manufacturer BMW in Eastern Bavaria, Germany and Rayong, Thailand, and considers their implications for regional development.},
  isbn = {00202754},
  pmid = {11224862},
  keywords = {Asia,Europe,global production networks,globalization,regional development},
  file = {C:\Users\csf14\Zotero\storage\JQ5KMFIS\Coe et al. - 2004 - G lobalizing Regional Development A Global Production Networks Perspective(2).pdf}
}

