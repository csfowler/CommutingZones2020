@article{Peck2013b,
  title = {For {{Polanyian}} Economic Geographies},
  author = {Peck, J},
  year = {2013},
  journal = {Environment and Planning A},
  volume = {45},
  number = {7},
  pages = {1545--1568},
  publisher = {{Pion Ltd}},
  abstract = {Karl Polanyi has been an influential but also somewhat elusive figure in economic geography. Best known for his evocative notion of social embeddedness, it is perhaps fitting that Polanyi's presence has been more metaphorical than substantive. The paper asks what a more engaged Polanyian economic geography might look like. Focusing on methodological affinities, a response is developed in terms of a commitment to the substantivist (as opposed to formal) analysis of actually existing economic formations, together with a more purposive embrace of institutionalism, holism, and comparativism.                Keywords: Karl Polanyi, economic geography, substantivism, institutionalism, comparative economy},
  keywords = {\#nosource,❓ Multiple DOI}
}

