@article{Hwang2014,
  title = {Divergent {{Pathways}} of {{Gentrification}}: {{Racial Inequality}} and the {{Social Order}} of {{Renewal}} in {{Chicago Neighborhoods}}.},
  author = {Hwang, Jackelyn and Sampson, Robert J},
  year = {2014},
  journal = {American Sociological Review},
  volume = {79},
  number = {4},
  issn = {00031224},
  doi = {10.1177/0003122414535774},
  abstract = {Gentrification has inspired considerable debate, but direct examination of its uneven evolution across time and space is rare. We address this gap by developing a conceptual framework on the social pathways of gentrification and introducing a method of systematic social observation using Google Street View to detect visible cues of neighborhood change. We argue that a durable racial hierarchy governs residential selection and, in turn, gentrifying neighborhoods. Integrating census data, police records, prior street-level observations, community surveys, proximity to amenities, and city budget data on capital investments, we find that the pace of gentrification in Chicago from 2007 to 2009 was negatively associated with the concentration of blacks and Latinos in neighborhoods that either showed signs of gentrification or were adjacent and still disinvested in 1995. Racial composition has a threshold effect, however, attenuating gentrification when the share of blacks in a neighborhood is greater than 40 percent. Consistent with theories of neighborhood stigma, we also find that collective perceptions of disorder, which are higher in poor minority neighborhoods, deter gentrification, while observed disorder does not. These results help explain the reproduction of neighborhood racial inequality amid urban transformation. [ABSTRACT FROM PUBLISHER]},
  keywords = {\#nosource}
}

@article{Hwang2015,
  title = {Gentrification in {{Changing Cities}}: {{Immigration}}, {{New Diversity}}, and {{Racial Inequality}} in {{Neighborhood Renewal}}},
  author = {Hwang, J.},
  year = {2015},
  month = jun,
  journal = {The ANNALS of the American Academy of Political and Social Science},
  volume = {660},
  number = {1},
  pages = {319--340},
  issn = {0002-7162},
  doi = {10.1177/0002716215579823},
  urldate = {2015-06-28},
  abstract = {This article examines how the rise of immigration and its associated racial and ethnic changes relate to gentrification. In the decades following the 1965 Hart-Celler Act, gentrification has occurred more in cities with high levels of immigration and in neighborhoods with higher levels of immigrants. These relationships, however, vary by the ways in which a city is racially segregated and by the extent to which its immigrant population has been incorporated. Using crime data, surveys, and new gentrification measures, this article compares Chicago, a highly segregated city and predominantly Hispanic immigrant destination, with Seattle, a predominantly white city with high levels of Asian immigration. The findings show that immigration and its correlates have distinct and evolving relationships with neighborhood changes that are embedded in the racial and immigrant histories of each city, and that gentrification perpetuates racial and ethnic inequality in both cities.},
  keywords = {\#nosource}
}

@article{Hwang2015a,
  title = {Residential {{Segregation}}, {{Housing Submarkets}}, and {{Spatial Analysis}}: {{St}}. {{Louis}} and {{Cincinnati}} as a {{Case Study}}},
  author = {Hwang, Sungsoon},
  year = {2015},
  journal = {Housing Policy Debate},
  volume = {25},
  number = {1},
  issn = {1051-1482},
  doi = {10.1080/10511482.2014.934703},
  abstract = {This article considers how spatial analysis of housing submarkets can advance research into residential segregation. While an emphasis on housing submarkets has been proposed as a new construct for modeling housing prices, its use in analyzing residential segregation has been limited. Recent advances in spatial analysis and geographic information systems present new opportunities for researchers to exploit the potential of housing submarkets as constructs that offer a more precise way to examine residential segregation. The article synthesizes literature related to residential segregation and housing submarkets and demonstrates how to delineate housing submarkets using publicly available data. It examines the spatial distribution of housing submarkets and how the socially disadvantaged are represented across housing submarkets in St. Louis, Missouri, and Cincinnati, Ohio, to conclude that St. Louis's housing market is more polarized and racially segregated than Cincinnati's. Spatial analysis of housing submarkets, in conjunction with archival analysis, provides a promising avenue for identifying residential segregation as a multidimensional phenomenon, and a means to explore local processes of urban inequality.},
  keywords = {\#nosource}
}

@book{Hwang2015b,
  title = {The {{Social Construction}} of a {{Gentrifying Neighborhood}}: {{Reifying}} and {{Redefining Identity}} and {{Boundaries}} in {{Inequality}}},
  author = {Hwang, J.},
  year = {2015},
  journal = {Urban Affairs Review},
  volume = {79},
  issn = {1078-0874},
  doi = {10.1177/1078087415570643},
  abstract = {This study draws upon cognitive maps and interviews with 56 residents living in a gentrifying area to examine how residents socially construct neighborhoods. Most minority respondents, regardless of socioeconomic status and years of residency, defined their neighborhood as a large and inclusive spatial area, using a single name and conventional boundaries, invoking the area's black cultural history, and often directly responding to the alternative way residents defined their neighborhoods. Both long-term and newer white respondents defined their neighborhood as smaller spatial areas and used a variety of names and unconventional boundaries that excluded areas that they perceived to have lower socioeconomic status and more crime. The large and inclusive socially constructed neighborhood was eventually displaced. These findings shed light on how the internal narratives of neighborhood identity and boundaries are meaningfully tied to a broader structure of inequality and shape how neighborhood identities and boundaries change or remain.},
  isbn = {1-07-808741-5},
  keywords = {\#nosource}
}

@article{Hwang2015c,
  title = {Racial and Spatial Targeting: {{Segregation}} and Subprime Lending within and across Metropolitan Areas},
  author = {Hwang, Jackelyn and Hankinson, Michael and Kreg Brown, Steven},
  year = {2015},
  journal = {Social Forces},
  volume = {93},
  number = {3},
  issn = {15347605},
  doi = {10.1093/sf/sou099},
  abstract = {Recent studies find that high levels of black-white segregation increased rates of foreclosures and subprime lending across US metropolitan areas during the housing crisis. These studies speculate that segregation created distinct geographic markets that enabled subprime lenders and brokers to leverage the spatial proximity of minorities to disproportionately target minority neighborhoods. Yet, the studies do not explicitly test whether the concentration of subprime loans in minority neighborhoods varied by segregation levels. We address this shortcoming by integrating neighborhood-level data and spatial measures of segregation to examine the relationship between segregation and subprime lending across the 100 largest US metropolitan areas. Controlling for alternative explanations of the housing crisis, we find that segregation is strongly associated with higher concentrations of subprime loans in clusters of minority census tracts but find no evidence of unequal lending patterns when)},
  isbn = {0037-7732},
  keywords = {\#nosource}
}

@article{hwang2016,
  title = {While {{Some Things Change}}, {{Some Things Stay The Same}}: {{Reflections}} on the {{Study}} of {{Gentrification}}},
  author = {Hwang, Jackelyn},
  year = {2016},
  journal = {City and Community},
  volume = {15},
  number = {3},
  issn = {15406040},
  doi = {10.1111/cico.12188},
  abstract = {Although we have been studying gentrification for more than 50 years now, gentrification seems to have become a hot topic of public concern only in recent years. The growing interest in the phenomenon reflects both its increasing prevalence and its changing na-ture. The term now conjures up connotations and stylized images of race and class, but these are often inconsistent with scholarly findings of gentrification. These inconsisten-cies between the past and present and between the public and academic understanding of the phenomenon bring us to an appropriate juncture to reflect on the scholarship on gentrification. For most of the twentieth century, the study of neighborhood change that dominated urban sociology explained neighborhood decline in central cities. With rapid urban growth in the first half of the twentieth century followed by the large-scale decline of manufacturing and suburbanization, the general trend of central city neighborhoods in the United States was indeed decline. It is no surprise, then, that theoretical understand-ings of neighborhood change largely focused on this particular direction of change. Early Chicago School scholars described the invasion and succession process by which the in-nermost central city neighborhoods continually remained home to those with the lowest socioeconomic status as cities grew and new immigrants arrived (Park et al. 1925), and a subsequent generation of urban scholars sought to explain the declining social condi-tions in the poor minority neighborhoods that expanded in the years after World War II (e.g., Massey and Denton 1993; Wilson 1987). Following the publications of Wilson's (1987) The Truly Disadvantaged and Massey and Denton's (1993) American Apartheid, a field of research blossomed, particularly within urban sociology, on the persistence of these poor minority neighborhoods. This research has made substantial progress over the last few decades in advancing our understanding of the causes and consequences of these neighborhoods. While the dominant trend of low-income, central city neighborhoods has cer-tainly been that of decline and persistence, gentrification{\textemdash}the socioeconomic upgrad-ing of previously low-income, central city neighborhoods 1 {\textemdash}has become increasingly widespread and has made its mark in many urban areas by now. Urban scholars began to take note of gentrification appearing across several cities over 50 years ago. Gentri-fication countered the dominant trend of low-income, central city neighborhoods, and forced scholars to rethink understandings of neighborhood change, which mainly ex-plained the decline of central city neighborhoods at the time. a break from suburbanization and homogeneity for many middle-and upper-class res-idents and a restructuring of central business districts and nearby areas. In response, scholars sought to explain the extent of this phenomenon and understand its causes and consequences. Over the years, research on gentrification made considerable progress. As gentrifica-tion took place in more cities and neighborhoods, scholarship on the subject increased and included many notable developments in the field (for comprehensive reviews, see Brown-Saracino 2010; Lees et al. 2008; Zukin 1987). Much of this progress can be at-tributed to scholars uncovering the " chaotic " (Rose 1984) nature of the process. Despite this progress, theoretical and empirical understandings of gentrification have not par-alleled those of studies of neighborhood decline and the persistence of neighborhood poverty and segregation. In an Annual Review of Sociology article on gentrification, Zukin (1987) describes an " empirical stalemate " in the field stemming from disagreement on the underlying struc-ture of gentrification. Since Zukin's (1987) publication, gentrification has taken on new and even more complex forms that vary across cities. Gentrification since the late 1990s reflects a new era of gentrification distinct from its past. Hackworth and Smith (2001) describe this gentrification as more intense and widespread, occurring beyond the cen-tral core of cities in economically riskier areas. Others emphasize the shift in gentrifi-cation's relationship to race (e.g., Goetz, 2011; Hyra, 2012) and the increasing role of state and corporate actors in facilitating contemporary gentrification (e.g., Hackworth, 2007). Despite our improved understanding of gentrification's complexity, the increasing complexity of gentrification itself has left us still in an " empirical stalemate. " As gentrification has become even more chaotic than before, a growing plurality of disciplines beyond sociology and geography, including economics, history, political science, and public health, to name a few, are studying it. Further, gentrification has become increasingly politicized in public discourse, and conceptual and methodologi-cal disagreements continue to plague the field. In the remainder of this essay, I propose approaches to studying gentrification that I believe can move research beyond this empir-ical stalemate. These approaches can help bridge past understandings of gentrification with present ones and can lead to improved empirical research that can inform public understandings and move policy solutions forward. First, studying the process of gentrification in relation to processes of neighborhood stability can advance the study of neighborhood dynamics more broadly. One way in which scholarship on gentrification has been limited is that it has generally remained isolated from the study of persistent poverty and segregation. This isolation of research agendas has limited theoretical progress. When gentrification was a relatively small-scale phenomenon occurring in select major cities and primarily near downtown areas, the separate development of gentrification research may have been sufficient. With gentrifi-cation's increased prevalence and growing public concern surrounding its implications for inequality, it no longer suffices to study gentrification separately from the persistence of poor minority neighborhoods. After all, neighborhoods are interdependent: Why some neighborhoods change{\textemdash} either upward or downward{\textemdash}and others remain the same is a social process in itself that shapes and is shaped by the social conditions of other neighborhoods. For exam-ple, gentrifiers 2 select neighborhoods depending on the ecological context within which they are choosing which neighborhoods to upgrade. In highly segregated cities, where 2},
